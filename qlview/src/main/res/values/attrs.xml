<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LabelTextView">
        <attr name="labelText" format="string"/>
        <attr name="symbolText" format="string"/>
        <attr name="text" format="string"/>
        <attr name="textSize" format="dimension"/>
        <attr name="size" format="dimension"/>
        <attr name="labelSize" format="dimension"/>
        <attr name="textMarginLeft" format="dimension"/>
        <attr name="symbolTextMarginLeft" format="dimension"/>
        <attr name="textColor" format="color"/>
        <attr name="color" format="color"/>
        <attr name="labelColor" format="color"/>
    </declare-styleable>
    <!--自定义圆头像属性-->

    <declare-styleable name="roundedimageview">
        <attr name="border_thickness" format="dimension" />
        <attr name="border_inside_color" format="color" />
        <attr name="border_outside_color" format="color"/>
    </declare-styleable>
</resources>